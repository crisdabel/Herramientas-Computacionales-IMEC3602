    "import random as r\n",
    "import math as m\n",
    "\n",
    "inside = 0\n",
    "n = 10000000\n",
    "\n",
    "\n",
    "for i in range(0,n):\n",
    "    x = r.random()**2\n",
    "    y = r.random()**2\n",
    "    \n",
    "    if m.sqrt(x+y) < 1.0:\n",
    "        inside += 1\n",
    "        \n",
    "pi = (float(inside) / n) *4\n",
    "\n",
    "print(pi)"
   ]
